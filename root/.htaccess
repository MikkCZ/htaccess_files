###
# security and other
###
Options -Indexes
<Files .htaccess>
	order allow,deny
	deny from all
</Files>
<Files .htpasswd>
	order allow,deny
	deny from all
</Files>
<Files ~ "^.*\.([Hh][Tt][Aa])">
	order allow,deny
	deny from all
</Files>
<Files .svn,error_log>
	order allow,deny
	deny from all
</Files>
<IfModule mod_headers.c>
	# Add SAMEORIGIN for non wp-admin sections also.
	Header add X-Frame-Options "SAMEORIGIN"
	# XSS protection
	Header add X-XSS-Protection "1; mode=block"
	# IE Specific header
	Header add X-Content-Type-Options "nosniff"
	# Remove Server Signature 
	Header unset Server
</IfModule>

###
# browser cache
###
<IfModule mod_expires.c>
	ExpiresActive On
	# default 10 minutes
		ExpiresDefault A600
	# HTML/PHP/plain/XHTML/XML 10 minutes
		ExpiresByType text/html A600
		ExpiresByType text/php A600
		ExpiresByType application/php A600
		ExpiresByType text/plain A600
		ExpiresByType application/xhtml+xml A600
		ExpiresByType text/xml A600
		ExpiresByType application/xml A600
		ExpiresByType application/x-xml A600
	# CSS 1 week
		ExpiresByType text/css A604800
	# JavaScript 1 week
		ExpiresByType text/javascript A604800
		ExpiresByType text/x-javascript A604800
		ExpiresByType application/javascript A604800
		ExpiresByType application/x-javascript A604800
	# images 1 week
		ExpiresByType image/bmp A604800
		ExpiresByType image/gif A604800
		ExpiresByType image/ico A604800
		ExpiresByType image/x-icon A604800
		ExpiresByType image/vnd.microsoft.icon A604800
		ExpiresByType image/jpg A604800
		ExpiresByType image/jpeg A604800
		ExpiresByType image/png A604800
	# fonts 1 week
		ExpiresByType application/font-otf A604800
		ExpiresByType application/x-font-otf A604800
		ExpiresByType application/font-ttf A604800
		ExpiresByType application/x-font-ttf A604800
		ExpiresByType application/font-woff A604800
		ExpiresByType application/x-font-woff A604800
		ExpiresByType application/vnd.ms-fontobject A604800
		ExpiresByType image/svg+xml A604800
	# other 1 week
		ExpiresByType application/x-shockwave-flash A604800
</IfModule>
<IfModule mod_headers.c>
	<FilesMatch "\\.(x?html?|php)$">
		Header set Cache-Control "private, max-age=600, must-revalidate"
	</FilesMatch>
	<FilesMatch "\\.(css)$">
		Header set Cache-Control "public, max-age=604800"
	</FilesMatch>
	<FilesMatch "\\.(js)$">
		Header set Cache-Control "private, max-age=604800"
	</FilesMatch>
	<FilesMatch "\\.(bmp|gif|ico|jpe?g|png)$">
		Header set Cache-Control "public, max-age=604800"
	</FilesMatch>
	# BEGIN Turn ETags Off
		Header unset Pragma
		FileETag None
		Header unset ETag
		Header unset Last-Modified
	# END Turn ETags Off
</IfModule>

# BEGIN Compress text files
<IfModule mod_deflate.c>
AddOutputFilterByType DEFLATE \
	text/html \
	text/xml \
	text/css \
	text/plain \
	image/svg+xml \
	application/xhtml+xml \
	application/xml \
	application/rdf+xml \
	application/rss+xml \
	application/atom+xml \
	text/javascript \
	text/ecmascript \
	application/javascript \
	application/x-javascript \
	application/ecmascript \
	application/json \
	application/x-font-ttf \
	application/x-font-otf \
	font/truetype \
	font/opentype \
	application/x-pointplus
</IfModule>
# END Compress text files

# allow keep-alive
<IfModule mod_headers.c>
	Header set Connection keep-alive
</IfModule>

